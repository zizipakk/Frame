    "jasmine-spec-reporter": "2.5.0",
    "json-loader": "^0.5.3",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
