{
  "name": "frame-ui",
  "version": "1.0.0",
  "main": "wwwroot/index.html",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {

    "start angular-cli": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",

    "ngc": "ngc -p ./tsconfig-aot.json",
    "start": "concurrently \"webpack-dev-server --inline --progress --port 4200\" \"dotnet run\" ",
    "webpack-dev": "set NODE_ENV=development && webpack",
    "webpack-production": "set NODE_ENV=production && webpack",
    "build-dev": "npm run webpack-dev",
    "build-production": "npm run ngc && npm run webpack-production",
    "watch-webpack-dev": "set NODE_ENV=development && webpack --watch --color",
    "watch-webpack-production": "npm run build-production --watch --color",
    "publish-for-iis": "npm run build-production && dotnet publish -c Release"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.2.4",
    "@angular/compiler": "2.2.4",
    "@angular/core": "2.2.4",
    "@angular/forms": "2.2.4",
    "@angular/http": "2.2.4",
    "@angular/platform-browser": "2.2.4",
    "@angular/platform-browser-dynamic": "2.2.4",
    "@angular/router": "3.2.4",

    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "zone.js": "^0.7.4",
    "ie-shim": "^0.1.0",

    "alertify.js": "^1.0.12",

    "font-awesome": "~4.7.0",
    "moment": "^2.17.1",
    "primeng": "^1.0.1"

  },
  "devDependencies": {
    "@angular/compiler-cli": "2.2.4",
    "@angular/platform-server": "~2.4.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular2-template-loader": "^0.5.0",
    "angular-cli": "1.0.0-beta.21",
    "awesome-typescript-loader": "^2.2.4",
    "clean-webpack-plugin": "^0.1.9",
    "codelyzer": "~1.0.0-beta.3",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^2.8.1",
