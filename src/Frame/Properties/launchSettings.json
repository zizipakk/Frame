{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:5200/", //IIS portconfig
      "sslPort": 0
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Frame": { //Kestrel portconfig in appSettings.json "server.urls" and static used in program.cs
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "http://localhost:5200/",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Docker": { //Docker portconfig in docker-compose.yml 5200:80 /public:private/, so EXPOSE in dockerfile not neccessary
      "launchBrowser": true,
      "launchUrl": "http://localhost:{ServicePort}/"
    }
  }
}